{
    "manifest_version":2,
    "name":"Kitten extension",
    "version":"1.0.0",
    "web_accessible_resources":[
        "*.jpg"
    ],
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["popup.js"]
        }
    ],
    "description":"insert kitten images",
    "icons":{
        "128":"ic128.png",
        "48":"ic48.png",
        "16":"ic16.png"
    },
    "browser_action":{
        "default_icon":"ic16.png",
        "default_popup":"popup.html"
    }
}