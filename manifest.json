{
    "manifest_version":2,
    "name":"Kitten extension",
    "version":"1.0.0",
    "description":"insert kitten images",
    "icons":{
        "128":"money128.png",
        "48":"money48.png",
        "16":"money16.png"
    },
    "browser_action":{
        "default_icon":"money16.png",
        "default_popup":"popup.html"
    },
    "options_page":"options.html",
    "background":{
        "scripts":["eventPage.js"],
        "persistent":false      
    },
    "permissions":[
        "storage",
        "notifications",
        "contextMenus"
    ]
}